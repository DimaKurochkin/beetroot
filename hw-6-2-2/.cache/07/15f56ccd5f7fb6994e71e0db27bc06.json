\n$H18: calcRem(18px);\r\n$H14: calcRem(14px);\r\n","scss\\components\\_mixins.scss":"@mixin text($font-size: $H24, $font-weight: 500, $font-color: $WHITE) {\r\n    font-size: $font-size ;\r\n    font-weight: $font-weight;\r\n    color: $font-color;\r\n}\r\n@mixin flex($jc: flex-start, $ai: stretch, $direction: raw, $wrap: nowrap) {\r\n    display: flex;\r\n    flex-direction: $direction;\r\n    justify-content: $jc;\r\n    align-items: $ai;\r\n    flex-wrap: $wrap;\r\n    @content;\r\n}\r\n\r\n\r\n@mixin flex-col($jc: flex-start, $ai: stretch, $direction: column, $wrap: nowrap) {\r\n    display: flex;\r\n    flex-direction: $direction;\r\n    justify-content: $jc;\r\n    align-items: $ai;\r\n    flex-wrap: $wrap;\r\n    @content;\r\n}\r\n\r\n\r\n@mixin box($size) {\r\n    width: $size;\r\n    height: $size;\r\n}","scss\\components\\_header.scss":".header{\r\n    padding: 22px;\r\n    width: 100%;\r\n    background-color: $GREY-LIGHT;\r\n    @include flex-col(flex-start, center);\r\n    &__title {\r\n        @include text($H24, 700, $HEADING-COLOR);\r\n        margin-bottom: 4px;\r\n    }\r\n}","scss\\components\\_subtitle.scss":".subtitle {\r\n   @include text($H18,500, $HEADING-COLOR);\r\n}","scss\\components\\_main.scss":"$PADDING: 40px;\r\n.main{\r\n    align-self: center;\r\n    padding: $PADDING;\r\n    width: 100%;\r\n    max-width: 800px + ($PADDING*2);\r\n    @include flex-col( );\r\n    &__list{\r\n        margin-bottom: 40px;\r\n    }\r\n}","scss\\components\\_shopping-list.scss":".shopping-list {\r\n    list-style: none;\r\n    &__item:nth-of-type(odd) {\r\n      background-color: $ODD-BG-COLOR;\r\n    }\r\n    &__item:nth-of-type(even) {\r\n      background-color: $EVEN-BG-COLOR;\r\n    }\r\n  } ","scss\\components\\_shopping-item.scss":".shopping-item {\r\n    display: block;\r\n    overflow: hidden;\r\n    padding: 16px;\r\n    @include flex(flex-start, center);\r\n    cursor: pointer;\r\n    transition: background-color 0.2s ease;\r\n    transition-property: background-color, border-color ;\r\n    transition-duration: 0.2s;\r\n    transition-timing-function: ease;\r\n    &:hover {\r\n        background: $HOVER-BG-COLOR;\r\n        border-top: 2px solid white;\r\n        border-bottom: 2px solid white;\r\n    }\r\n    &:active {\r\n        background: $ACTIVE-BG-COLOR;\r\n    }\r\n    &__input {\r\n        @include box(24px);\r\n        margin-right: 20px;\r\n        flex-shrink: 0;\r\n        &:checked + .shopping-item__content {\r\n            text-decoration: line-through;\r\n            .shopping-item__name{\r\n                color: $TEXT-SECONDARY-COLOR;\r\n                   \r\n            }\r\n           \r\n        }\r\n    }\r\n    &__content {\r\n        @include flex(flex-start, center);\r\n    }\r\n    &__name {\r\n        @include text($H20, 400, $WHITE);\r\n    }\r\n    &__extra {\r\n        @include text($H14, 400, $TEXT-SECONDARY-COLOR);\r\n        position: relative;\r\n        top: 1px;\r\n        display: inline-block;\r\n    \r\n    &:before {\r\n        content: '(';\r\n    }\r\n    &:after {\r\n        content: ')';\r\n    }\r\n    }\r\n}","scss\\components\\_footer.scss":".footer {\r\n    padding: 22px ;\r\n    background-color: $GREY-LIGHT;\r\n    text-align: center;\r\n}"},"lineCount":null}},"error":null,"hash":"26b4b7fe9998111bd0897e5c45d648f1","cacheData":{"env":{}}}